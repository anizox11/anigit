[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/anizox11
[access "refs/for/refs/*"]
	read = group user/anizox11
	push = group user/anizox11
	label-Code-Review = -2..+2 group user/anizox11
	label-Verified = -1..+1 group user/anizox11
	rebase = group user/anizox11
	removeReviewer = group user/anizox11
	submit = group user/anizox11
[access "refs/heads/*"]
	read = group user/anizox11
	create = group user/anizox11
	push = +force group user/anizox11
	pushMerge = group user/anizox11
[access "refs/tags/*"]
	push = group user/anizox11
	push = +force group user/anizox11
